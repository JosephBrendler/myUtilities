#!/bin/bash
# launch_cmd_file - auto-generated by qemu-image-launch 19 Feb 2025
old_dir=/home/joe/myUtilities/qemu-tools
cd /home/joe/My_bcm2709-rpi-2-b_project/images
source /usr/local/sbin/script_header_brendlefly
message "launch_cmd_file now working in $(pwd)"
sh_countdown 1
echo
qemu-system-arm \
 -name raspiOS2q1 \
 -M raspi2b \
 -m 1G \
 -smp 4 \
 -drive if=sd,file=raspiOS2q1.img,format=raw \
 -dtb bcm2709-rpi-2-b.dtb \
 -kernel kernel7.img \
 -append "console=ttyAMA0,115200 \
    console=tty1 \
    root=PARTUUID=96c7abe7-02 \
    rw \
    earlyprintk \
    loglevel=8 \
    rootfstype=ext4 \
    rootwait \
    fsck.repair=yes \
    net.ifnames=0" \
 -audio none \
 &

cd ${old_dir}
message "launch_cmd_file now working in $(pwd)"
message "qemu launched. Hit ENTER"

read throwaway
