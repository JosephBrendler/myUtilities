# first, force correct permissions on all package files
chown -R portage:portage /usr/armv6j-hardfloat-linux-gnueabi/usr/portage/packages/
find /usr/armv6j-hardfloat-linux-gnueabi/usr/portage/packages/ -type d -exec chmod 755 '{}' \;
find /usr/armv6j-hardfloat-linux-gnueabi/usr/portage/packages/ -type f -exec chmod 644 '{}' \;


